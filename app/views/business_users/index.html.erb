<h1>Listing business_users</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Email</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Title</th>
      <th>Password</th>
      <th>Password digest</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @business_users.each do |business_user| %>
      <tr>
        <td><%= business_user.business %></td>
        <td><%= business_user.email %></td>
        <td><%= business_user.first_name %></td>
        <td><%= business_user.last_name %></td>
        <td><%= business_user.title %></td>
        <td><%= business_user.password %></td>
        <td><%= business_user.password_digest %></td>
        <td><%= link_to 'Show', business_user %></td>
        <td><%= link_to 'Edit', edit_business_user_path(business_user) %></td>
        <td><%= link_to 'Destroy', business_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business user', new_business_user_path %>
