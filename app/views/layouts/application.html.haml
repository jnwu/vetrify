    %meta{ charset: "utf-8" }
    %meta{ name: 'author', content: 'Vetrify' }
    %meta{ name: "msvalidate.01" content: "28C3843B08B787662C61E5A61F6CF6E6" }

  %body
    - if content_for?(:nav)
      %nav.m-nav
        = yield(:nav)

    = yield
